{
    "manifest_version": 3,
    "name": "mdash mod by taw",
    "version": "1.1.2",
    "description": "A minimal dashboard replacing the \"New Tab\" page which syncs with your bookmarks.",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "permissions": [
        "bookmarks",
        "contextMenus"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "chrome_url_overrides": {
        "newtab": "html/dashboard.html"
    }
}
